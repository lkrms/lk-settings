{
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "[json]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[nginx]": {
        "editor.defaultFormatter": "ahmadalli.vscode-nginx-conf"
    },
    "[php]": {
        "editor.autoClosingBrackets": "languageDefined"
    },
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "bashIde.highlightParsingErrors": false,
    "beautify.config": {
        "break_chained_methods": true,
        "end_with_newline": true,
        "indent_body_inner_html": true,
        "indent_handlebars": true,
        "indent_head_inner_html": true,
        "indent_size": 2,
        "max_preserve_newlines": 2,
        "operator_position": "after-newline",
        "space_after_anon_function": true,
        "space_after_named_function": true,
        "space_before_conditional": true,
        "wrap_line_length": 80
    },
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.autoIndent": "advanced",
    "editor.comments.ignoreEmptyLines": false,
    "editor.comments.insertSpace": false,
    "editor.cursorBlinking": "solid",
    "editor.cursorSurroundingLines": 2,
    "editor.dragAndDrop": false,
    "editor.fontFamily": "'JetBrains Mono', 'Source Code Pro', Menlo, Monaco, 'Courier New', monospace",
    "editor.lineHeight": 19,
    "editor.quickSuggestions": {
        "comments": true
    },
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "selection",
    "editor.rulers": [
        80
    ],
    "editor.suggest.localityBonus": true,
    "emmet.triggerExpansionOnTab": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.decorations.badges": false,
    "explorer.incrementalNaming": "smart",
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "**/.git/info/attributes": "properties",
        "**/.nginx/*/*": "nginx",
        "**/bash/*": "shellscript",
        "**/git/ignore": "ignore",
        "**/git/{config*,attributes}": "properties",
        "**/polkit-1/rules.d/*.rules": "javascript",
        "**/unison/*": "properties",
        "*-lua": "lua",
        "*.desktop": "properties",
        "*.service": "properties",
        "*.socket": "properties",
        "*.target": "properties",
        "*crontab*": "properties",
        ".{bash_login,bash_profile,bashrc,profile,xsessionrc}": "shellscript"
    },
    "files.exclude": {
        "**/extjs/build/**": true
    },
    "files.watcherExclude": {
        "**/wp-content/uploads/**": true
    },
    "git.confirmSync": true,
    "git.countBadge": "off",
    "git.enableStatusBarSync": false,
    "git.ignoreLimitWarning": true,
    "gitlens.advanced.fileHistoryFollowsRenames": true,
    "gitlens.blame.format": "${message|40?} ${date|14-}",
    "gitlens.codeLens.enabled": false,
    "gitlens.codeLens.scopes": [
        "document"
    ],
    "gitlens.currentLine.format": "${author}, ${date} • ${message}",
    "gitlens.hovers.avatars": false,
    "gitlens.menus": {
        "editor": {
            "blame": false,
            "clipboard": true,
            "compare": true,
            "details": false,
            "history": false,
            "remote": false
        },
        "editorGroup": false,
        "editorTab": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": true
        },
        "explorer": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": true
        },
        "scmGroup": {
            "compare": true,
            "openClose": true,
            "stash": true,
            "stashInline": true
        },
        "scmItem": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": true,
            "stash": true
        }
    },
    "gitlens.statusBar.format": "${author}, ${date} • ${message}",
    "gitlens.statusBar.reduceFlicker": true,
    "html.autoClosingTags": false,
    "intelephense.stubs": [
        "apache",
        "bcmath",
        "bz2",
        "calendar",
        "com_dotnet",
        "Core",
        "ctype",
        "curl",
        "date",
        "dba",
        "dom",
        "enchant",
        "exif",
        "FFI",
        "fileinfo",
        "filter",
        "fpm",
        "ftp",
        "gd",
        "gettext",
        "gmp",
        "hash",
        "iconv",
        "imap",
        "intl",
        "json",
        "ldap",
        "libxml",
        "mbstring",
        "memcache",
        "memcached",
        "meta",
        "mssql",
        "mysqli",
        "oci8",
        "odbc",
        "openssl",
        "pcntl",
        "pcre",
        "PDO",
        "pdo_ibm",
        "pdo_mysql",
        "pdo_pgsql",
        "pdo_sqlite",
        "pgsql",
        "Phar",
        "posix",
        "pspell",
        "readline",
        "Reflection",
        "session",
        "shmop",
        "SimpleXML",
        "snmp",
        "soap",
        "sockets",
        "sodium",
        "SPL",
        "sqlite3",
        "standard",
        "superglobals",
        "sysvmsg",
        "sysvsem",
        "sysvshm",
        "tidy",
        "tokenizer",
        "xml",
        "xmlreader",
        "xmlrpc",
        "xmlwriter",
        "xsl",
        "Zend OPcache",
        "zip",
        "zlib"
    ],
    "javascript.autoClosingTags": false,
    "javascript.validate.enable": false,
    "keyboard.dispatch": "keyCode",
    "material-icon-theme.languages.associations": {
        "php": "php_elephant"
    },
    "nginx-conf-hint.syntax": "sublime",
    "nightswitch.autoSwitch": false,
    "nightswitch.themeDay": "Ayu Light Bordered",
    "nightswitch.themeNight": "Dracula Soft",
    "poor-mans-t-sql-formatter.std.indent": "    ",
    "poor-mans-t-sql-formatter.std.trailingCommas": false,
    "powershell.startAutomatically": false,
    "python.formatting.autopep8Args": [
        "--aggressive"
    ],
    "search.exclude": {
        "**/*.log": true
    },
    "search.followSymlinks": false,
    "shellcheck.customArgs": [
        "-x"
    ],
    "shellcheck.disableVersionCheck": true,
    "shellcheck.exclude": [
        "SC1007",
        "SC1090",
        "SC1091",
        "SC2001",
        "SC2015",
        "SC2016",
        "SC2027",
        "SC2029",
        "SC2030",
        "SC2031",
        "SC2034",
        "SC2046",
        "SC2064",
        "SC2086",
        "SC2119",
        "SC2120",
        "SC2128",
        "SC2153",
        "SC2154",
        "SC2206",
        "SC2207"
    ],
    "shellcheck.executablePath": "shellcheck",
    "shellcheck.run": "onSave",
    "shellformat.flag": "-i 4",
    "standard.enable": false,
    "telemetry.enableTelemetry": false,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.cwd": "${fileDirname}",
    "terminal.integrated.detectLocale": "off",
    "terminal.integrated.enableFileLinks": false,
    "terminal.integrated.env.linux": {
        "LC_BYOBU": "0"
    },
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.lineHeight": 1.2,
    "terminal.integrated.scrollback": 10000,
    "terminal.integrated.sendKeybindingsToShell": true,
    "terminal.integrated.shell.osx": "/opt/homebrew/bin/bash",
    "terminal.integrated.showExitAlert": false,
    "trailing-spaces.highlightCurrentLine": false,
    "typescript.autoClosingTags": false,
    "update.mode": "none",
    "update.showReleaseNotes": false,
    "window.newWindowDimensions": "inherit",
    "window.restoreFullscreen": true,
    "window.title": "${dirty}${rootName}${separator}${activeEditorShort}${separator}${appName}",
    "window.titleBarStyle": "custom",
    "workbench.editor.closeEmptyGroups": false,
    "workbench.editor.closeOnFileDelete": true,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.revealIfOpen": true,
    "workbench.enableExperiments": false,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.startupEditor": "newUntitledFile",
    "xml.format.emptyElements": "collapse",
    "xml.format.enforceQuoteStyle": "preferred",
    "xml.format.preserveAttributeLineBreaks": true,
    "xml.format.preservedNewlines": 0
}
