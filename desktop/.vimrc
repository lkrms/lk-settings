unlet! skip_defaults_vim
source $VIMRUNTIME/defaults.vim

set mouse=
set ignorecase     " Case-insensitive search     (same as `:set ic`,    disable with `:set noic`)
set incsearch      " Highlight next match        (same as `:set is`,    disable with `:set nois`)
set nohlsearch     " Don't highlight every match (same as `:set nohls`, enable with  `:set hls`)
colorscheme desert
