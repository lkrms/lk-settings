-A lk_output -p udp -m udp --dport 123 -j ACCEPT
-A lk_output -j ACCEPT
-A lk_reject -m mark ! --mark 0x1/0x1 -m limit --limit 12/min -j LOG --log-prefix "rejected: "
-A lk_reject -j lk_reject_quiet
-A lk_reject_quiet -p udp -m udp -j REJECT --reject-with icmp-port-unreachable
-A lk_reject_quiet -p tcp -m tcp -j REJECT --reject-with tcp-reset
-A lk_reject_quiet -j REJECT --reject-with icmp-proto-unreachable
-A lk_related -p icmp -j ACCEPT
-A lk_related -p tcp -m tcp --tcp-flags RST RST -j ACCEPT
-A lk_related -p tcp -m helper --helper ftp -m tcp --dport 1024:65535 -j ACCEPT
-A lk_related -m limit --limit 12/min -j LOG --log-prefix "RELATED not expected: "
-A lk_related -j ACCEPT
-A lk_trusted -j ACCEPT
-A r_invalid -p tcp -m tcp --tcp-flags RST RST -j DROP
-A r_invalid -m mark ! --mark 0x1/0x1 -m limit --limit 12/min -j LOG --log-prefix "dropped (INVALID): "
-A r_invalid -j DROP
-A r_new_not_syn -m mark ! --mark 0x1/0x1 -m limit --limit 12/min -j LOG --log-prefix "rejected (NEW not SYN): "
-A r_new_not_syn -p tcp -j REJECT --reject-with tcp-reset
-A z_lan_to_node -p tcp -m tcp --dport 25 -j ACCEPT
-A z_lan_to_node -p tcp -m tcp --dport 53 -j ACCEPT
-A z_lan_to_node -p tcp -m tcp --dport 80 -j ACCEPT
