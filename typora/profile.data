{
    "SmartyPantsOnRendering": true,
    "apply_image_move_for_web": false,
    "autoEscapeImageURL": true,
    "auto_expand_block": false,
    "backgroundColor": "#FFFFFF",
    "can_collapse_outline_panel": false,
    "copy_markdown_by_default": true,
    "customFontSize": 14,
    "customWordsPerMinute": true,
    "custom_image_save_location": "./.${filename}.assets",
    "enableAutoSave": true,
    "enable_diagram": true,
    "enable_highlight": true,
    "enable_inline_math": true,
    "enable_subscript": true,
    "enable_superscript": true,
    "ignoreLineBreak": true,
    "image_save_location": "custom",
    "indentSize": 4,
    "line_ending_crlf": false,
    "match_pari_markdown": true,
    "noWarnigForDeleteFile": true,
    "noWarnigForMoveFile": true,
    "ol_style": 1,
    "preLinebreakOnExport": false,
    "preset_spell_check": "disabled",
    "prettyIndent": false,
    "restoreWhenLaunch": "0",
    "send_usage_info": false,
    "showStatusBar": true,
    "show_line_numbers_for_fence": true,
    "smartDash": true,
    "smartQuote": true,
    "strict_mode": true,
    "theme": "github.css",
    "useCustomFontSize": false,
    "useTreeStyle": true,
    "use_relative_path_for_img": true,
    "wordsPerMinute": 240
}
