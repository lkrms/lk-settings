{
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "bashIde.highlightParsingErrors": false,
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.autoIndent": "advanced",
    "editor.comments.ignoreEmptyLines": false,
    "editor.comments.insertSpace": false,
    "editor.copyWithSyntaxHighlighting": false,
    "editor.cursorBlinking": "solid",
    "editor.dragAndDrop": false,
    "editor.fontFamily": "'JetBrains Mono', 'Source Code Pro', Menlo, Monaco, 'Courier New', monospace",
    "editor.lineHeight": 19,
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "selection",
    "editor.rulers": [
        80
    ],
    "emmet.triggerExpansionOnTab": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.decorations.badges": false,
    "explorer.incrementalNaming": "smart",
    "extensions.ignoreRecommendations": true,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "files.associations": {
        "**/bash/*": "shellscript",
        "**/unison/*": "properties",
        "*-lua": "lua",
        "*.desktop": "properties",
        "*.service": "properties",
        "*.socket": "properties",
        ".{bash_login,bash_profile,bashrc,profile,xsessionrc}": "shellscript"
    },
    "files.watcherExclude": {
        "**/wp-content/uploads/**": true
    },
    "git.confirmSync": true,
    "git.countBadge": "off",
    "git.enableStatusBarSync": false,
    "git.ignoreLimitWarning": true,
    "gitlens.blame.format": "${message|40?} ${date|14-}",
    "gitlens.codeLens.enabled": false,
    "gitlens.currentLine.format": "${author}, ${date} • ${message}",
    "gitlens.hovers.avatars": false,
    "gitlens.menus": {
        "editor": {
            "blame": false,
            "clipboard": true,
            "compare": true,
            "details": false,
            "history": false,
            "remote": false
        },
        "editorGroup": false,
        "editorTab": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": true
        },
        "explorer": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": true
        },
        "scmGroup": {
            "compare": true,
            "openClose": true,
            "stash": true,
            "stashInline": true
        },
        "scmItem": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": true,
            "stash": true
        }
    },
    "gitlens.statusBar.format": "${author}, ${date} • ${message}",
    "gitlens.statusBar.reduceFlicker": true,
    "html.autoClosingTags": false,
    "intelephense.format.enable": false,
    "intelephense.stubs": [
        "apache",
        "bcmath",
        "bz2",
        "calendar",
        "com_dotnet",
        "Core",
        "ctype",
        "curl",
        "date",
        "dba",
        "dom",
        "enchant",
        "exif",
        "FFI",
        "fileinfo",
        "filter",
        "fpm",
        "ftp",
        "gd",
        "gettext",
        "gmp",
        "hash",
        "iconv",
        "imap",
        "interbase",
        "intl",
        "json",
        "ldap",
        "libxml",
        "mbstring",
        "mcrypt",
        "memcache",
        "memcached",
        "meta",
        "mssql",
        "mysqli",
        "oci8",
        "odbc",
        "openssl",
        "pcntl",
        "pcre",
        "pdo_ibm",
        "pdo_mysql",
        "pdo_pgsql",
        "pdo_sqlite",
        "PDO",
        "pgsql",
        "Phar",
        "posix",
        "pspell",
        "readline",
        "recode",
        "Reflection",
        "regex",
        "session",
        "shmop",
        "SimpleXML",
        "snmp",
        "soap",
        "sockets",
        "sodium",
        "SPL",
        "sqlite3",
        "standard",
        "superglobals",
        "sybase",
        "sysvmsg",
        "sysvsem",
        "sysvshm",
        "tidy",
        "tokenizer",
        "wddx",
        "xml",
        "xmlreader",
        "xmlrpc",
        "xmlwriter",
        "xsl",
        "Zend OPcache",
        "zip",
        "zlib"
    ],
    "javascript.autoClosingTags": false,
    "javascript.validate.enable": false,
    "keyboard.dispatch": "keyCode",
    "material-icon-theme.languages.associations": {
        "php": "php_elephant"
    },
    "nightswitch.autoSwitch": false,
    "php.suggest.basic": false,
    "poor-mans-t-sql-formatter.std.indent": "    ",
    "poor-mans-t-sql-formatter.std.trailingCommas": false,
    "python.formatting.autopep8Args": [
        "--aggressive"
    ],
    "search.followSymlinks": false,
    "search.usePCRE2": true,
    "shellcheck.customArgs": [
        "-x"
    ],
    "shellcheck.disableVersionCheck": true,
    "telemetry.enableTelemetry": false,
    "terminal.explorerKind": "external",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.cwd": "${fileDirname}",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.lineHeight": 1.2,
    "terminal.integrated.scrollback": 10000,
    "trailing-spaces.highlightCurrentLine": false,
    "typescript.autoClosingTags": false,
    "update.mode": "none",
    "window.newWindowDimensions": "inherit",
    "window.restoreFullscreen": true,
    "window.title": "${dirty}${rootName}${separator}${activeEditorShort}${separator}${appName}",
    "window.titleBarStyle": "custom",
    "workbench.editor.closeOnFileDelete": true,
    "workbench.editor.focusRecentEditorAfterClose": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.revealIfOpen": true,
    "workbench.enableExperiments": false,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.startupEditor": "newUntitledFile",
    "xml.format.enforceQuoteStyle": "preferred",
    "xml.format.preservedNewlines": 0
}
